{
  "name": "genie_deck",
  "version": "0.2.2",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "release": "vue-cli-service build --modern --non-interactive && yarn deploy",
    "lint": "vue-cli-service lint",
    "deploy": "gsutil -m -h \"Cache-Control: no-store\" rsync -d -r dist gs://genie-deck/",
    "ngrok": "ngrok http -host-header=rewrite 8080"
  },
  "dependencies": {
    "@bitforgehq/genie-api-client": "^1.0.3",
